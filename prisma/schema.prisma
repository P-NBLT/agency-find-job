// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Agency{
id Int @id @default(autoincrement())
name String
city String
region String
size String
website String
logo String
}

model User{
  id Int @id @default(autoincrement())
  email String @unique
  password String
  firstName String
  lastName String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}